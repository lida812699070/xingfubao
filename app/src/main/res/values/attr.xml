<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DefaultEditText">
        <attr name="edit_input_type" format="enum">
            <enum name="edit_input_type_text" value="0" />
            <enum name="edit_input_type_password" value="1" />
            <enum name="edit_input_type_number" value="2" />
        </attr>
        <attr name="edit_input_max_length" format="integer" />
        <attr name="edit_input_hint" format="string" />
    </declare-styleable>

    <declare-styleable name="MyToolbarBar">
        <attr name="str_title_toolbar" format="string" />
        <attr name="right_image_toolbar" format="reference" />
    </declare-styleable>

</resources>